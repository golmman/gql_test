    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "compose": "rover supergraph compose --config ./src/gateway/supergraph.yaml > ./src/gateway/supergraph_schema.gql",
        "start": "./start.sh",
        "start:gateway": "yarn build && node dist/gateway/gateway.js",
        "start:cart": "yarn build && node dist/cart/cart.js",
        "start:product": "yarn build && node dist/product/product.js"
    },
    "dependencies": {
        "@apollo/gateway": "^2.0.5",
        "@apollo/subgraph": "^2.0.5",
        "apollo-server": "^3.10.0",
        "graphql": "^16.5.0"
    },
    "devDependencies": {
        "@apollo/rover": "^0.8.1",
        "prettier": "^2.7.1",
        "typescript": "^4.7.4"
    }
}
